{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=db.xxx.supabase.co;Database=postgres;Username=postgres;Password=your-password;Port=5432;SSL Mode=Require;Trust Server Certificate=true",
    "Redis": "localhost:6379"
  },
  "Supabase": {
    "Url": "https://xxx.supabase.co",
    "Key": "your-supabase-anon-key",
    "ServiceKey": "your-supabase-service-key"
  },
  "ExternalAPIs": {
    "AlphaVantage": {
      "BaseUrl": "https://www.alphavantage.co/query",
      "ApiKey": "your-alpha-vantage-key",
      "TimeoutSeconds": 10
    },
    "TwelveData": {
      "BaseUrl": "https://api.twelvedata.com",
      "ApiKey": "your-twelve-data-key",
      "TimeoutSeconds": 10
    },
    "NSE": {
      "BaseUrl": "https://www.nseindia.com/api",
      "TimeoutSeconds": 10
    },
    "Gemini": {
      "ApiKey": "your-gemini-api-key",
      "Model": "gemini-pro"
    }
  },
  "Cache": {
    "DefaultExpirationMinutes": 5,
    "StockQuoteExpirationSeconds": 10,
    "IndicesExpirationSeconds": 5,
    "HistoricalDataExpirationMinutes": 60
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:8080",
      "http://localhost:5173",
      "http://localhost:3000",
      "https://your-app.vercel.app"
    ]
  },
  "RateLimit": {
    "PermitLimit": 100,
    "WindowMinutes": 1
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/stocktracker-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  }
}
